{
  "root": "apps/deployment",
  "sourceRoot": "apps/deployment",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/deployment",
        "main": "apps/deployment/main.ts",
        "tsConfig": "apps/deployment/tsconfig.app.json",
        "assets": ["apps/deployment/cdktf.json"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "deployment:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/deployment/**/*.ts"]
      }
    },
    "tf-build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.app.json"
          },
          {
            "command": "cp cdktf.json ../../dist/apps/deployment/"
          }
        ],
        "parallel": false,
        "cwd": "apps/deployment"
      }
    },
    "tf-get": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cdktf get",
        "cwd": "dist/apps/deployment"
      }
    },
    "tf-synth": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cdktf synth",
        "cwd": "dist/apps/deployment"
      }
    }
  },
  "tags": []
}
